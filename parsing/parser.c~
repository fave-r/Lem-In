#include "lem_in.h"

t_lem		*parsing()
{
  t_lem		*ptr;
  int		ants;
  char		*tmp;

  ptr = NULL;
  while ((tmp = get_next_line(0)))
    {
      ants = atoi(tmp);
      if (ants <= 0)
	bad_ants();
    }
}
